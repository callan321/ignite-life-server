{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Secret": "YOUR_SECRET_KEY_which_should_be_long_and_secure",
    "Issuer": "YourApp",
    "Audience": "YourAppClient",
    "AccessTokenExpiryMinutes": 15,
    "SlidingRefreshTokenExpiryHours": 1,
    "RememberMeRefreshTokenExpiryDays": 7
  }
}
